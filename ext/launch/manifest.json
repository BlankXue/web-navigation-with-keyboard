{
  "manifest_version": 2,

  "name": "Keyboard launch",
  "description": "Simple and useful, one key to launch, so beautiful. 简单与实用，一键直达，很是优雅。",
  "version": "1.0",
  "icons":{
    "16":"icon16.png",
    "48":"icon48.png",
    "128":"icon128.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "options.html"
  },
  "permissions": [
    "alarms",
    "http://*/",
    "https://*/"
  ],
  //if you want to use ctrl + T or new tap button you can try it
  //"chrome_url_overrides": {
  //  "newtab": "jump.html"
  //},
  "options_page": "options.html",
  "background": {
    "scripts": [
      "jquery191.js",
      "base.js",
      "background.js"
    ],
    "persistent": false
  },
  "commands": {
    "newTab": {
      "suggested_key": {
        "default": "Ctrl+E"
      },
      "description": "新建标签页"
    }
  }
}